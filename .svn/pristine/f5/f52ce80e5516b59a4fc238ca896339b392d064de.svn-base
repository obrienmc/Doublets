import java.util.LinkedList;
import java.util.Queue;


public class QueueChainManager extends ChainManager {

	Queue<Chain> queue;
	
	public QueueChainManager(){
		queue = new LinkedList<Chain>();
	}
	@Override
	public void add(Chain chain) {
		queue.offer(chain);

	}

	@Override
	public Chain next() {
		return queue.poll();
	}

}
